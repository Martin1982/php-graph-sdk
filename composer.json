{
    "name": "martin1982/facebook-graph-sdk",
    "description": "Facebook SDK for PHP",
    "keywords": ["facebook", "sdk"],
    "type": "library",
    "homepage": "https://github.com/Martin1982/php-graph-sdk",
    "license": "CC-BY-4.0",
    "authors": [
        {
            "name": "Martin de Keijzer",
            "homepage": "https://martindekeijzer.nl"
        },
        {
            "name": "Facebook",
            "homepage": "https://github.com/facebook/php-graph-sdk/contributors"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.4",
        "php-http/discovery": "^1.13",
        "php-http/guzzle7-adapter": "^0.1.1",
        "php-http/message": "^1.11",
        "phpspec/prophecy-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Facebook\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Facebook\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.x-dev"
        }
    }
}
